{
  "name": "vuero",
  "version": "2.6.1",
  "private": true,
  "type": "module",
  "license": "SEE LICENSE IN LICENSE.md",
  "author": {
    "name": "cssninjaStudio <hello@cssninja.io> (https://cssninja.io)"
  },
  "engines": {
    "node": ">=14.18.0",
    "npm": ">=7"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint -e -V "
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.scss": [
      "stylelint",
      "prettier --write"
    ],
    "*.md": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.vue": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "scripts": {
    "dev": "run-p dev:vite dev:json-server",
    "dev:vite": "vite",
    "dev:json-server": "json-server --read-only --routes ./json-server/routes.json --port 8080 --watch ./json-server/db.json",
    "build": "run-s build:*",
    "build:update-bulma-colors": "bulma-css-vars",
    "build:vite": "vite build",
    "ssr:build": "run-s build:update-bulma-colors ssr:build:*",
    "ssr:build:client": "vite build --ssrManifest --outDir dist/client",
    "ssr:build:server": "vite build --ssr src/entry-server.ts --outDir dist/server",
    "ssr:dev": "jiti server.ts",
    "ssr:serve": "cross-env NODE_ENV=production jiti server.ts",
    "ssr:preview": "run-p ssr:serve preview:json-server",
    "preview": "run-p preview:vite preview:json-server",
    "preview:vite": "http-server dist -s --proxy http://localhost:5000? -p 5000",
    "preview:json-server": "json-server -q --read-only --routes ./json-server/routes.json --host 0.0.0.0 --port 8080 ./json-server/db.json",
    "lint": "run-s lint:doc:fix lint:eslint:fix lint:stylelint:fix lint:prettier:fix",
    "lint:prettier": "prettier --check \"./src/**/*.(ts|vue|css|scss)\"",
    "lint:prettier:fix": "prettier --write \"./src/**/*.(ts|vue|css|scss)\"",
    "lint:eslint": "eslint --ext .ts,.vue ./src",
    "lint:eslint:fix": "eslint --fix --ext .ts,.vue ./src",
    "lint:stylelint": "stylelint ./src/**/*.{vue,css,scss}",
    "lint:stylelint:fix": "stylelint ./src/**/*.{vue,css,scss} --fix",
    "lint:doc:fix": "run-s lint:doc:fix:*",
    "lint:doc:fix:eslint": "eslint --fix --ext .md ./documentation",
    "lint:doc:fix:prettier": "prettier --write \"./documentation/**/*.(md)\"",
    "test": "run-p test:lint test:tsc test:e2e",
    "test:tsc": "vue-tsc --noEmit",
    "test:lint": "run-p lint:eslint lint:stylelint lint:prettier",
    "test:e2e": "cypress run",
    "test:unit": "echo \"[warning] no unit test implemented yet\"",
    "cypress": "cypress"
  },
  "dependencies": {
    "@ckeditor/ckeditor5-build-classic": "35.4.0",
    "@ckeditor/ckeditor5-vue": "4.0.1",
    "@cssninja/bulma-css-vars": "0.9.2",
    "@iconify/iconify": "3.0.1",
    "@mapbox/mapbox-gl-geocoder": "5.0.1",
